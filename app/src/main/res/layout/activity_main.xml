<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button_mainactivity_keychain"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias=".2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="@onKeyChainClicked"
        android:text="Key Chain"/>

    <Button
        android:id="@+id/button_mainactivity_keystore"
        app:layout_constraintTop_toBottomOf="@id/button_mainactivity_keychain"
        app:layout_constraintRight_toRightOf="@id/button_mainactivity_keychain"
        app:layout_constraintLeft_toLeftOf="@id/button_mainactivity_keychain"
        android:layout_marginTop="50dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="keySotre"/>

    <Button
        android:id="@+id/button_mainactivity_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onGotoTestClicked"
        android:text="Test"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/button_mainactivity_keystore"
        app:layout_constraintRight_toRightOf="@id/button_mainactivity_keystore"
        app:layout_constraintLeft_toLeftOf="@id/button_mainactivity_keystore"/>


</androidx.constraintlayout.widget.ConstraintLayout>